{
  "family": "%TASK_FAMILY%",
  "containerDefinitions": [
      {
          "image": "%AWS_ACCOUNT_ID%.dkr.ecr.us-east-1.amazonaws.com/%IMAGE_NAME%:%IMAGE_VERSION%",
          "name": "%SERVICE_NAME%",
          "cpu": %CPU_CONTAINER%,
          "memory": %MEMORY_CONTAINER%,
          "essential": true,
          "portMappings": [
              {
                  "containerPort": %PORT%,
                  "hostPort": %PORT%,
                  "protocol": "tcp"
              }
          ],
          "entryPoint": [],
          "environment": [
            {
              "name": "stage",
              "value": "/dev"
            },
            {
              "name": "SERVER_PORT",
              "value": "8090"
            },
            {
              "name": "PRODUCT_SERVICE_ENDPOINT",
              "value": "https://63f577683f99f5855dc1d874.mockapi.io/aceleraTi"
            },
            {
              "name": "NAME_DATASOURCE",
              "value": "stock-scarface"
            },
            {
              "name": "PORT_DATASOURCE",
              "value": "3306"
            },
            {
              "name": "HOST_DATASOURCE",
              "value": "aceleratidb.c6mcx0paxyp1.us-east-1.rds.amazonaws.com"
            },
            {
              "name": "JWTEXPIRATION",
              "value": "4000"
            }
          ],
          "secrets": [
            {
              "name": "URL_DATASOURCE",
              "valueFrom": "arn:aws:ssm:us-east-1:052727711491:parameter/db/url/scarface"
            },
            {
              "name": "PASS_DATASOURCE",
              "valueFrom": "arn:aws:ssm:us-east-1:052727711491:parameter/db/password"
            },
            {
              "name": "JWTSECRET",
              "valueFrom": "arn:aws:ssm:us-east-1:052727711491:parameter/config/keysecret"
            },
            {
              "name": "USER_DATASOURCE",
              "valueFrom": "arn:aws:ssm:us-east-1:052727711491:parameter/db/username"
            }
          ],
          "mountPoints": [],
          "volumesFrom": [],
          "logConfiguration": {
              "logDriver": "awslogs",
              "options": {
                  "awslogs-group": "dev-logs",
                  "awslogs-region": "us-east-1",
                  "awslogs-stream-prefix": "dev"
              }
          }
      }
  ],
  "networkMode": "awsvpc",
  "requiresCompatibilities": [
      "EC2"
  ],
  "taskRoleArn": "arn:aws:iam::%AWS_ACCOUNT_ID%:role/ecsTaskExecutionRoleAceleraTI",
  "executionRoleArn": "arn:aws:iam::%AWS_ACCOUNT_ID%:role/ecsTaskExecutionRoleAceleraTI"
}